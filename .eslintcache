={isFilled} isFocused={isFocused}>\n      {Icon && <Icon size={20} />}\n      <input\n        defaultValue={defaultValue}\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        ref={inputRef}\n        {...rest}\n      />\n\n      {error && (\n        <Error title={error}>\n          <FiAlertCircle color=\"#c53030\" size={20} />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/Input/styles.ts",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/hooks/auth.tsx",["175","176","177","178","179","180","181","182","183"],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/services/api.ts",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/ToastContainer/index.tsx",["184"],{"ruleId":"185","severity":1,"message":"186","line":1,"column":17,"nodeType":"187","messageId":"188","endLine":1,"endColumn":28},{"ruleId":"185","severity":1,"message":"189","line":3,"column":10,"nodeType":"187","messageId":"188","endLine":3,"endColumn":21},{"ruleId":"190","severity":2,"message":"191","line":23,"column":3,"nodeType":"187","messageId":"192","endLine":23,"endColumn":15},{"ruleId":"190","severity":2,"message":"193","line":24,"column":3,"nodeType":"187","messageId":"192","endLine":24,"endColumn":24},{"ruleId":"194","severity":1,"message":"195","line":46,"column":32,"nodeType":"196","messageId":"197","endLine":46,"endColumn":35,"suggestions":"198"},{"ruleId":"194","severity":1,"message":"195","line":52,"column":34,"nodeType":"196","messageId":"197","endLine":52,"endColumn":37,"suggestions":"199"},{"ruleId":"190","severity":2,"message":"191","line":69,"column":11,"nodeType":"187","messageId":"192","endLine":69,"endColumn":23},{"ruleId":"190","severity":2,"message":"193","line":70,"column":11,"nodeType":"187","messageId":"192","endLine":70,"endColumn":32},{"ruleId":"200",