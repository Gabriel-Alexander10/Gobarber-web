obarber-web/src/hooks/toast.tsx",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/Input/index.tsx",["119","120"],"import React, {\n  InputHTMLAttributes,\n  useEffect,\n  useRef,\n  useState,\n  useCallback,\n} from 'react';\nimport { IconBaseProps } from 'react-icons';\nimport { FiAlertCircle } from 'react-icons/fi';\nimport { useField } from '@unform/core';\n\nimport { Container, Error } from './styles';\n\ninterface InputProps extends InputHTMLAttributes<HTMLInputElement> {\n  name: string;\n  icon?: React.ComponentType<IconBaseProps>;\n}\n\nconst Input: React.FC<InputProps> = ({ name, icon: Icon, ...rest }) => {\n  const inputRef = useRef<HTMLInputElement>(null);\n  const [isFocused, setIsFocused] = useState(false);\n  const [isFilled, setIsFilled] = useState(false);\n  const { fieldName, error, registerField, defaultValue } = useField(name);\n\n  const handleInputFocus = useCallback(() => {\n    setIsFocused(true);\n  }, []);\n\n  const handleInputBlur = useCallback(() => {\n    setIsFocused(false);\n\n    setIsFilled(!!inputRef.current?.value);\n    // duas negações, \"\" é false, então vira !!false =>  !true = false,\n    // \"algo\" é true, então vira !!true => !false = true;\n  }, []);\n\n  useEffect(() => {\n    registerField({\n      name: fieldName,\n      ref: inputRef.current, // é nesse current que fica o input\n      path: 'value', // onde fica o valor do input (getElement....().value)\n    });\n  }, [fieldName, registerField]);\n\n  return (\n    <Container isErrored={!!error} isFilled={isFilled} isFocused={isFocused}>\n      {Icon && <Icon size={20} />}\n      <input\n        defaultValue={defaultValue}\n        onFocus={handleInputFocus}\n        onBlur={handleInputBlur}\n        ref={inputRef}\n        {...rest}\n      />\n\n      {error && (\n        <Error title={error}>\n          <FiAlertCircle color=\"#c53030\" size={20} />\n        </Error>\n      )}\n    </Container>\n  );\n};\n\nexport default Input;\n","/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/ToastContainer/Toast/index.tsx",["121","122"],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/ToastContainer/Toast/styles.ts",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/hooks/index.tsx",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/hooks/auth.tsx",["123","124"],"import React, { createContext, useCallback, useContext, useState } from 'react';\nimport api from '../services/api';\n\ninterface AuthState {\n  token: string;\n  user: object;\n}\n\ninterface SingInCredentials {\n  email: string;\n  password: string;\n}\n\ninterface AuthContextData {\n  user: object;\n  singIn(credentials: SingInCredentials): Promise<void>;\n  signOut(): void;\n}\n\nconst AuthContext = createContext<AuthContextData>({} as AuthContextData);\n\nconst AuthProvider: React.FC = ({ children }) => {\n  const [data, setData] = useState<AuthState>(() => {\n    const token = localStorage.getItem('@GoBarber:token');\n    const user = localStorage.getItem('@GoBarber:user');\n\n    if (token && user) {\n      return { token, user: JSON.parse(user) };\n    }\n\n    return {} as AuthState;\n  });\n\n  const singIn = useCallback(async ({ email, password }) => {\n    const response = await api.post('/sessions', {\n      email,\n      password,\n    });\n\n    const { user, token } = response.data;\n\n    localStorage.setItem('@GoBarber:token', token);\n    localStorage.setItem('@GoBarber:user', JSON.stringify(user));\n\n    setData({ token, user });\n  }, []);\n\n  const signOut = useCallback(() => {\n    localStorage.removeItem('@GoBarber:token');\n    localStorage.removeItem('@GoBarber:user');\n\n    setData({} as AuthState);\n  }, []);\n\n  return (\n    <AuthContext.Provider value={{ user: data.user, singIn, signOut }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nfunction useAuth(): AuthContextData {\n  const context = useContext(AuthContext);\n\n  if (!context) {\n    throw new Error('useAuth must be used within an AuthProvider.');\n  }\n\n  return context;\n}\n\nexport { AuthProvider, useAuth };\n",["125","126","127","128","129"],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/Button/styles.ts",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/Button/index.tsx",["130"],"import React, { ButtonHTMLAttributes } from 'react';\nimport { Container } from './styles';\n\ntype ButtonProps = ButtonHTMLAttributes<HTMLButtonElement>;\n\nconst Button: React.FC<ButtonProps> = ({ children, ...rest }) => (\n  <Container type=\"button\" {...rest}>\n    {children}\n  </Container>\n);\n\nexport default Button;\n","/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/ToastContainer/styles.ts",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/Tooltip/index.tsx",[],"/home/gabriel/Área de Trabalho/RocketSeat/modulo3/Aulas/gobarber-web/src/components/ToastContainer/index.tsx",["131"],"import React from 'react';\nimport { useTransition } from 'react-spring';\n\nimport Toast from './Toast';\n\nimport { Container } from './styles';\nimport { ToastMessage } from '../../hooks/toast';\n\ninterface ToastContainerProps {\n  messages: ToastMessage[];\n}\n\nconst ToastContainer: React.FC<ToastContainerProps> = ({ messages }) => {\n  const messagesWithTransictions = useTransition(\n    messages,\n    message => message.id,\n    {\n      from: { right: '-120%', opacity: 0 },\n      enter: { right: '0%', opacity: 1 },\n      leave: { right: '-120%', opacity: 0 },\n    },\n  );\n\n  return (\n    <Container>\n      {messagesWithTransictions.map(({ item, key, props }) => (\n        <Toast key={key} style={props} message={item} />\n      ))}\n    </Container>\n  );\n};\n\nexport default ToastContainer;\n","/home/gabriel/Área de Trabalho/RocketSeat/modulo3/A